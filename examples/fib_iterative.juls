exit :: (return_value: s32) {}

fib :: (n: u64) -> u64
{
    a := 0;
    b := 1;

    for (i : u64 = 0; i < n; i += 1)
    {
        c := a + b;
        a = b;
        b = c;
    }

    return a;
}

main :: ()
{
    // This gets truncated to a byte,
    // so the return value should be 225.
    exit(cast(s32) fib(47));
}
